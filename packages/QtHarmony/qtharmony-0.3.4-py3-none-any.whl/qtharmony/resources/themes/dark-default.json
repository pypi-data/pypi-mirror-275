{
    "Info": {
        "name": "Dark-Default",
        "author": null
    },

    "MainWindow": {
        "object-name": "QMainWindow#main-window",
        
        "": {
            "background-color": "#252525",
            "border": "none",
            "border-radius": "0px"
        }
    },

    "PushButton": {
        "object-name": "QPushButton#button",

        "": {
            "background-color": "#405cf5",
            "border": "none",
            "border-radius": "6px"
        },

        ":hover": {
            "background-color": "#3045B8"
        },

        ":pressed": {
            "background-color": "#283A9A"
        },

        ":disabled": {
            "background-color": "gray",
            "color": "lightgray"
        }
    },

    "DropDownMenu": {
        "object-name": "QComboBox#drop-down-menu",

        "": {
            "background-color": "#fbfbfb",
            "color": "#1A3B5D",
            "border": "none",
            "border-radius": "6px"
        },

        ":hover": {
            "border-color": "#6185DF"
        },

        " QAbstractItemView": {
            "background-color": "#fbfbfb"
        },

        "::drop-down": {
            "border": "none"
        },

        ":disabled": {
            "background-color": "gray",
            "color": "lightgray"
        }
    },

    "Entry": {
        "object-name": "QLineEdit#entry",

        "": {
            "border": "none",
            "background-color": "#fbfbfb",
            "color": "#1A3B5D",
            "border-radius": "6px"
        },

        ":hover": {
            "border-color": "#6185DF"
        },

        ":focus": {
            "border-color": "#405cf5"
        },

        ":disabled": {
            "background-color": "gray",
            "color": "lightgray"
        }
    },

    "DigitalEntry": {
        "object-name": "QSpinBox#digital-entry",

        "": {
            "color": "#1A3B5D",
            "background-color": "#fbfbfb",
            "border": "1px solid #adb5bd",
            "border-radius": "6px"
        },

        ":hover": {
            "border-color": "#6185DF"
        },

        ":focus": {
            "border-color": "#405cf5"
        }
    }, 

    "RadioButton": {
        "object-name": "QRadioButton#radio-button",

        "": {
            "color": "#fbfbfb"
        },

        "::indicator": {
            "border": "1px solid #0166fc",
            "border-radius": "7px"
        },

        "::indicator:checked:enabled": {
            "background-color": "#0166fc"
        },

        "::indicator:checked:disabled": {
            "background-color": "lightgray"
        },

        ":disabled": {
            "color": "gray"
        },

        "::indicator:disabled": {
            "border-color": "gray"
        }
    },

    "CheckBox": {
        "object-name": "QCheckBox#check-box",

        "": {
            "color": "#fbfbfb"
        },

        "::indicator": {
            "border": "1px solid #0166fc",
            "border-radius": "3px"
        },

        "::indicator:checked:enabled": {
            "background-color": "#0166fc"
        },

        "::indicator:checked:disabled": {
            "background-color": "lightgray"
        },

        ":disabled": {
            "color": "gray"
        },

        "::indicator:disabled": {
            "border-color": "gray"
        }
    }, 

    "GroupBox": {
        "object-name": "QGroupBox#group-box",

        "": {
            "background-color": "#323232",
            "border-radius": "6px",
            "border": "none"
        },

        "::title": {
            "color": "#fbfbfb",
            "background-color": "#404040",
            "border-radius": "6px",
            "subcontrol-position": "top",
            "margin-top": "5px",
            "min-height": "30px",
            "padding-left": "20px",
            "padding-right": "20px"
        }
    },

    "Frame": {
        "object-name": "Frame#frame",

        "": {
            "background-color": "#323232",
            "border-radius": "6px",
            "border": "none"
        }
    },

    "Label": {
        "object-name": "QLabel#label",

        "": {
            "color": "#fbfbfb"
        }
    },

    "Splitter": {
        "object-name": "QSplitter#splitter"
    },

    "TextBox": {
        "object-name": "QPlainTextEdit#text-box",

        "": {
            "padding": "5px",
            "background-color": "#404040",
            "border-radius": "6px"
        }
    },

    "WidgetsList": {
        "object-name": "QListWidget#widgets-list",

        "": {
            "background-color": "#404040",
            "border-radius": "6px",
            "border": "none"
        }, 

        "::item": {
            "min-height": "100px"
        }
    },

    "ScrollBar": {
        "object-name": "QScrollBar#scroll-bar",

        "": {
            "background-color": "#404040",
            "border-radius": "4px",
            "width": "10px"
        },
        
        ":handle": {
            "background-color": "#525252",
            "border-radius": "4px"
        },
        
        ":handle:hover": {
            "background-color": "#757575"
        },
        
        ":handle:pressed": {
            "background-color": "#808080"
        },
        
        "::add-page, ::sub-page": {
            "background": "#404040",
            "border": "none"
        }
    }
}